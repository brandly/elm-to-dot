{
  "name": "elm-to-dot",
  "version": "0.4.2",
  "license": "MIT",
  "scripts": {
    "build": "elm make src/Main.elm --optimize --output dist/index.js",
    "prepublishOnly": "npm run build && npm run minify",
    "minify": "uglifyjs --compress --mangle --output dist/index.js -- dist/index.js"
  },
  "devDependencies": {
    "elm": "0.19.1-5",
    "uglify-js": "3.16.1"
  },
  "files": [
    "*.js",
    "dist/"
  ],
  "bin": {
    "elm-to-dot": "./bin.js"
  }
}
