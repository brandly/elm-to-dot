{
  "name": "elm-to-dot",
  "version": "0.3.1",
  "license": "MIT",
  "scripts": {
    "build": "elm make src/Main.elm --optimize --output dist/index.js",
    "cli": "npm run build && node index.js",
    "prepublish": "npm run build && npm run minify",
    "minify": "uglifyjs --compress --mangle --output dist/index.js -- dist/index.js"
  },
  "devDependencies": {
    "elm": "0.19.1-3",
    "uglify-js": "3.9.1"
  },
  "files": [
    "*.js",
    "dist/"
  ],
  "bin": {
    "elm-to-dot": "./bin.js"
  }
}
